{
    "name": "marathon",
    "version": "1.0.0",
    "description": "An API built with NodeJS, Express, MongoDB and TypeScript",
    "main": "build/server.js",
    "scripts": {
        "pretest": "tsc --build tsconfig.test.json",
        "posttest": "rimraf spec/js",
        "test": "istanbul cover --include-all-sources true spec/specs.js -x **/spec/js/spec/** -x **/build/** && istanbul check-coverage --statements 0 --functions 0 --lines 0",
        "lint": "tslint --config ./tslint.json --project ./tsconfig.json --force --format stylish",
        "build": "npm run lint && webpack --config webpack.config.js --mode production",
        "start": "node ./build/server.js",
        "start:dev": "npm run build && npm run start"
    },
    "tslintConfig": {
        "env": {
            "browser": true,
            "node": true
        }
    },
    "keywords": [
        "fse"
    ],
    "author": "Raja, Vetri, Sudhakar",
    "license": "ISC",
    "dependencies": {
        "@types/cookie-parser": "^1.4.1",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.11.1",
        "@types/express-fileupload": "^0.4.0",
        "@types/jasmine": "^3.3.12",
        "@types/request": "^2.48.1",
        "@types/supertest": "^2.0.7",
        "@types/xlsx": "0.0.36",
        "atob": "^2.1.2",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.2",
        "cookie-parser": "^1.4.4",
        "dotenv": "^7.0.0",
        "express": "^4.16.3",
        "express-fileupload": "^1.1.2-alpha.1",
        "istanbul": "^0.4.5",
        "jasmine": "^3.3.1",
        "jasmine-console-reporter": "^3.1.0",
        "jasmine-node": "^1.16.2",
        "jasmine-supertest": "^1.0.0",
        "jsonwebtoken": "^8.4.0",
        "mongoose": "^5.4.12",
        "nodemon": "^1.18.9",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.0",
        "rimraf": "^2.6.3",
        "supertest": "^4.0.2",
        "tsc": "^1.20150623.0",
        "typegoose": "^5.5.0",
        "typescript-string-operations": "^1.3.1",
        "xlsx": "^0.14.1"
    },
    "devDependencies": {
        "@types/atob": "^2.1.1",
        "@types/bcrypt-nodejs": "0.0.30",
        "@types/jsonwebtoken": "^8.3.0",
        "@types/mongoose": "^5.3.12",
        "ts-loader": "^5.3.3",
        "ts-node": "^8.0.3",
        "tslint": "^5.15.0",
        "typescript": "^3.4.1",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0"
    }
}
